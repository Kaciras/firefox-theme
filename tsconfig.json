{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "Node",
		"strict": true,
		"useUnknownInCatchVariables": false,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"removeComments": true
	}
}
